  	<servlet-name>controladorTipoProducto</servlet-name>
  	<display-name>controladorTipoProducto</display-name>
  	<description></description>
  	<servlet-class>Controlador.controladorTipoProducto</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>controladorCliente</servlet-name>
  	<url-pattern>/controladorCliente</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>controladorUsuario</servlet-name>
  	<url-pattern>/controladorUsuario</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>controladorProducto</servlet-name>
  	<url-pattern>/controladorProducto</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>controladorTipoProducto</servlet-name>
  	<url-pattern>/controladorTipoProducto</url-pattern>
